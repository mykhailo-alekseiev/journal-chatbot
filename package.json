    "@vitejs/plugin-react": "5.1.2",
    "dotenv-cli": "11.0.0",
    "oxfmt": "0.24.0",
    "oxlint": "1.39.0",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
